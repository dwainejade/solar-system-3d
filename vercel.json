{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "routes": [
    {
      "src": "/main",
      "dest": "/main"
    },
    {
      "src": "/experiments",
      "dest": "/experiments"
    },
    {
      "src": "/explore",
      "dest": "/explore"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}